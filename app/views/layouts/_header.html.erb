<h1>Header</h1>

<% if user_signed_in? %>
  <h3>Hello, <%= current_user.full_name %></h3>
  <p><%= link_to "Edit", edit_user_registration_path %></p>
  <p><%= button_to "Sign Out", destroy_user_session_path, method: :delete %></p>
<% else %>
  <p><%= link_to "Sign In", new_user_session_path %></p>
  <p><%= link_to "Sign Up", new_user_registration_path %></p>
<% end %>

<p style="color:green;"><%= notice %></p>
<p style="color:red;"><%= alert %></p>
